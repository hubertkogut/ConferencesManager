@model IEnumerable<ConferencesProject.Models.ApplicationUser>

@{
    ViewBag.Title = "Users list";
}

<h2>Users list</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>
    </tr>

@foreach (var item in Model) {
    <tr class="UsersListItem">
        <td>
            @Html.DisplayFor(modelItem => item.Email)
        </td>
    </tr>
}

</table>
